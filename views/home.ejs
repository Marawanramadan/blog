<%- include('partials/header') %>

<h1>Home</h1> 
<p><%= startingContent %></p>

<% if(locals.post){ %>
    <% post.forEach((item) => { %>
        
            <h2><%= item.title %></h2>
            <p><%= item.post.substr(0,100) +"..." %> <a href="/posts/<%= item.title %>">Read more</a></p>
        
    <% }) %>    
<% } %>

<% include Partials/footer %>
